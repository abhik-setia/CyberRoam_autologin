{
  "manifest_version": 2,
  "name": "CyberRoam Autologin",
  "version": "1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["js/jquery-3.2.1.min.js"]
  }
],
  "icons":{
    "16": "Icons/laptop.png",
    "64":"Icons/laptop_64.png"
},
  "browser_action": {
  "default_icon":"Icons/laptop.png",
  "default_popup":"popup.html",
  "default_title":"We have your back"
},
"background": {
  "scripts": ["js/jquery-3.2.1.min.js","background.js"]
},
"permissions":[
]
}